# Production API Configuration for app.floworx-iq.com
REACT_APP_API_URL=https://app.floworx-iq.com/api

# Disable treating warnings as errors in production builds
GENERATE_SOURCEMAP=false
CI=false
NODE_ENV=production

# Disable ESLint during build to prevent initialization errors
DISABLE_ESLINT_PLUGIN=true
ESLINT_NO_DEV_ERRORS=true
TSC_COMPILE_ON_ERROR=true
