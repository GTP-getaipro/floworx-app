{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "project": ["backend/**/*.js"],
  "ignore": [
    "**/__tests__/**",
    "backend/**/test/**",
    "backend/**/fixtures/**",
    "backend/**/coverage/**",
    "**/__deprecated__/**",
    "backend/node_modules/**"
  ],
  "entry": [
    "backend/app.js",
    "backend/server.js"
  ],
  "ignoreDependencies": [
    "dotenv",
    "nodemon",
    "jest",
    "supertest",
    "eslint",
    "prettier"
  ]
}
