{
  "name": "floworx-verification-system",
  "version": "1.0.0",
  "description": "Comprehensive verification system for FloWorx application - catches and resolves issues automatically",
  "main": "index.js",
  "bin": {
    "floworx-verify": "./index.js"
  },
  "scripts": {
    "verify": "node index.js",
    "verify:quick": "node index.js --mode=quick",
    "verify:fix": "node index.js --fix",
    "verify:monitor": "node index.js --mode=monitor",
    "verify:report": "node index.js --report=html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "verification",
    "testing",
    "monitoring",
    "floworx",
    "automation",
    "quality-assurance"
  ],
