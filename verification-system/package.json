  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GTP-getaipro/floworx-app.git"
  },
  "bugs": {
    "url": "https://github.com/GTP-getaipro/floworx-app/issues"
