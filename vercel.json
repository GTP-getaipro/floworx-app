{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "framework": "create-react-app"
      }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/frontend/build/static/$1"
    },
    {
      "src": "/manifest.json",
      "dest": "/frontend/build/manifest.json"
    },
    {
      "src": "/favicon.ico",
      "dest": "/frontend/build/favicon.ico"
    },
    {
      "src": "/robots.txt",
      "dest": "/frontend/build/robots.txt"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/frontend/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/index.html"
    }
  ],
  "env": {
    "CI": "false",
    "GENERATE_SOURCEMAP": "false",
    "NODE_ENV": "production",
    "REACT_APP_API_URL": "https://floworx-gxl5ke7q0-floworxdevelopers-projects.vercel.app/api"
  },
  "build": {
    "env": {
      "CI": "false",
      "GENERATE_SOURCEMAP": "false",
      "NODE_ENV": "production",
      "REACT_APP_API_URL": "https://floworx-gxl5ke7q0-floworxdevelopers-projects.vercel.app/api"
    }
  },
  "regions": ["iad1"],
  "cleanUrls": true,
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}
